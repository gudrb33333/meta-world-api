#Datasource Configuration
spring:
  datasource:
    url: <need value>
    username: <need value>
    password: <need value>
  #JPA Configuration
  jpa:
    generate-ddl: false
    hibernate:
      ddl-auto: validate
    properties:
      hibernate:
        show_sql: true
        format_sql: true

logging.level:
  org.hibernate.SQL: debug
  org.hibernate.type: trace

#ssl
server:
  ssl:
    enable: true
    key-store: <need value>
    key-store-password: <need value>
    key-store-type: PKCS12
    key-alias: System

frontend:
  domain: <need value>

# AWS S3 bucket Information
cloud:
  aws:
    s3:
      bucket: <need value>
    region:
      static: <need value>
    stack:
      auto: false
    credentials:
      accessKey: <need value>
      secretKey: <need value>
    cloudFront:
      keyPairId: <need value>
      distributionDomain: <need value>
